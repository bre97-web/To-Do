    "autoprefixer": "^10.4.14",
    "electron": "^25.2.0",
    "electron-builder": "^24.4.0",
    "electron-vite": "^1.0.24",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.1",
    "moment": "^2.29.4",
    "postcss": "^8.4.24",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "5.1",
    "vite": "^4.3.9",

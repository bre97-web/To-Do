{
  "name": "to-do",
  "version": "1.0.0-pre.0",
  "author": "bre97-web",
  "description": "Written by Vue 3 and Electron with Vite.",
  "private": false,
  "main": "./out/main/index.js",
  "scripts": {
    "dev:web": "vite",
    "build:web": "run-p type-check build-only",
    "preview:web": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "dev:electron": "electron-vite dev",
    "preview:electron": "electron-vite preview",
    "build": "electron-vite build",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@fontsource/noto-sans-sc": "^4.5.12",
    "@fontsource/roboto": "^4.5.8",
    "lit": "^2.7.4"
  },
  "devDependencies": {
    "@electron/notarize": "^1.2.3",
    "@material/web": "^1.0.0-pre.8",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^20.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/tsconfig": "^0.3.2",
    "autoprefixer": "^10.4.14",
    "electron": "^24.2.0",
    "electron-builder": "^24.4.0",
    "electron-vite": "^1.0.22",
    "eslint": "^8.40.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-vue": "^9.11.1",
    "express": "^4.18.2",
    "less": "^4.1.3",
    "material-icons": "^1.13.6",
    "moment": "^2.29.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "pubsub": "^3.2.1",
    "rollup": "^3.21.6",
    "tailwindcss": "^3.3.2",
    "ts-loader": "^9.4.2",
    "tslint": "^6.1.3",
    "tslint-loader": "^3.5.4",
    "typescript": "^5.0.4",

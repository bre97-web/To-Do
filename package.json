  "description": "Written by Vue 3 and Electron with Vite.",
  "private": true,
  "main": "./out/main/index.js",
  "scripts": {
    "type-check": "vue-tsc --noEmit",
    "dev:web": "vite",
    "build:web": "run-p type-check build-only",
    "preview:web": "vite preview",
    "dev:electron": "electron-vite dev",
    "preview:electron": "electron-vite preview",
    "format": "prettier --write .",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build-only && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "build-only": "vite build",
    "build": "electron-vite build"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^1.0.2",
    "@fontsource/noto-sans-sc": "^5.0.4",
    "@fontsource/roboto": "^5.0.4",
    "@headlessui/vue": "^1.7.14",
    "lit": "^2.7.6",
    "material-symbols": "^0.9.0",
    "ninja-keys": "^1.2.2",
    "pinia": "^2.1.4",
    "pinia-plugin-persistedstate": "^3.1.0"
  },
  "devDependencies": {
    "@electron/notarize": "^2.1.0",
    "@material/web": "^1.0.0-pre.12",
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/node": "^20.4.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
